<UserControl x:Class="ReferenceConfigurator.templates.SaveDataControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ReferenceConfigurator.templates"
             xmlns:proxy="clr-namespace:ReferenceConfigurator.views.utils"
             xmlns:hc="https://handyorg.github.io/handycontrol">
    <StackPanel>

        <Button Style="{StaticResource ButtonPrimary}"
                Command="{Binding DeletLogosCommand}"
                Content="Delet Saved Logos"
                Margin="10">
        </Button>
        <Button Style="{StaticResource ButtonPrimary}"
                Command="{Binding DeletOnePagerCommand}"
                Content="Delet Saved OnePagers"
                Margin="10">
        </Button>
        <Button Style="{StaticResource ButtonPrimary}"
                Command="{Binding RefreshSavedTemplates}"
                Content="Refresh Saved Templates"
                Margin="10"
                />
        <Button Style="{StaticResource ButtonPrimary}"
                Command="{Binding RefreshSearchCommand}"
                Content="Refresh Search"
                Margin="10" />
    </StackPanel>
</UserControl>
