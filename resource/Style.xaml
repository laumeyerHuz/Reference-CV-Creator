<ResourceDictionary x:Class="ReferenceConfigurator.resource.Style"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:hc="https://handyorg.github.io/handycontrol"
                    xmlns:local="using:ReferenceConfigurator.resource">

    <Style x:Key="TextBlockFabricIcons" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="pack://application:,,,/ReferenceConfigurator;component/resource/fabric-icons.ttf#Fabric MDL2 Assets"/>
        <Setter Property="FontSize" Value="14"/>
    </Style>

</ResourceDictionary>
